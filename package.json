{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-react": "^7.16.7",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.22.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "quill": "^1.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.8.0",
    "react-quilljs": "^1.2.17",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "recharts": "^2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint:check": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint --fix src --ext .js,.jsx",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^7.2.1",
    "enzyme": "^3.11.0",
    "prettier": "^2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*styles.{js,jsx}",
      "!src/**/index.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!src/assets/**",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0
      }
    },
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
